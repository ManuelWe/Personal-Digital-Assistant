{
  "name": "gunter-backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www | pino-pretty",
    "start-dev": "nodemon 'node ./bin/www | pino-pretty' --ignore db/",
    "lint": "eslint .",
    "test": "jest"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "axios": "^0.19.2",
    "db-stations": "^3.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "geolib": "^3.2.1",
    "helmet": "^3.21.3",
    "lowdb": "^1.0.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.28",
    "node-ical": "^0.10.0",
    "node-schedule": "^1.3.2",
    "openweather-apis": "^4.2.0",
    "pino": "^5.17.0",
    "pino-pretty": "^3.6.1",
    "web-push": "^3.4.3"
  },
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "supertest": "^4.0.2"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFiles": [
      "dotenv/config"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/coverage/",
      "/tests/"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.js",
      "!**/node_modules/**"
    ]
  }
}
