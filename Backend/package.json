{
  "name": "gunter-backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www | pino-pretty",
    "start-dev": "nodemon 'node ./bin/www | pino-pretty' --ignore db/",
    "lint": "eslint .",
    "test": "jest --forceExit"
  },
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^2.1.2",
    "@hapi/joi": "^17.1.0",
    "axios": "^0.19.2",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "helmet": "^3.21.3",
    "install": "^0.13.0",
    "lowdb": "^1.0.0",
    "moment": "^2.24.0",
    "node-ical": "^0.10.0",
    "node-schedule": "^1.3.2",
    "openweather-apis": "^4.2.0",
    "pino": "^5.17.0",
    "pino-pretty": "^3.6.0",
    "web-push": "^3.4.3"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/coverage/",
      "/tests/"
